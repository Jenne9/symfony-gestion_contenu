<form method="post" action="{{ path('app_admin_carousel_delete', {'id': carousel.id}) }}" onsubmit="return confirm('Supprimer carousel ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ carousel.id) }}">
    <button title="Supprimer" class="btn btn-danger mt-3"><i class="align-middle" data-feather="trash-2"></i></button>
</form>
